	</Style>

	<Style TargetType="{x:Type TextBox}">
		<Setter Property="Background" Value="#FFFFFFFF"/>
		<Setter Property="BorderThickness" Value="0,1,0,1"/>
		<Setter Property="BorderBrush" Value="#FF333333"/>
